{
    "manifest_version": 3,
    "name": "Toolset",
    "version": "0.0.1",
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "scripting",
        "clipboardRead",
        "clipboardWrite",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "48": "icon.png"
    }
}